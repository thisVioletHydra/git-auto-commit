{
  "name": "git-autocommit",
  "displayName": "Git Autocommit",
  "description": "âš¡ pwsh script to automatically commit to the repository",
  "version": "1.0.5",
  "main": "index.ps1",
  "scripts": {
    "line::autopush": " ____________ ðŸš€ AUTOPUSH ðŸš€ ____________ ",
    "autopush": "pwsh -Command . ${env:userprofile}\\.vscode\\scripts\\index.ps1",
    "line::push": " ____________ ðŸš€ PUSH ðŸš€ ____________ ",
    "push": "yarn npm-run-all run:push::*",
    "run:push::patch": "yarn version --patch",
    "run:push::status": "git status -s",
    "run:push::add": "git add . ",
    "run:push::autopush": "yarn autopush",
    "run:push::push": "git push --follow-tags",
    "line::fetch": " ____________ ðŸš€ FETCH ðŸš€ ____________ ",
    "fetch": "yarn npm-run-all run:fetch::*",
    "run:fetch::fetch": "git fetch",
    "run:fetch::merge": "git merge",
    "run:fetch::yarn": "yarn",
    "line::up": " ____________ ðŸš€ UP ðŸš€ ____________ ",
    "up": "yarn-check -u && yarn",
    "up:latest": "yarn upgrade-interactive --latest",
    "line::main": " ____________ ðŸš€ MAIN ðŸš€ ____________ ",
    "killnode": "taskkill /f /im node.exe /im RuntimeBroker.exe"
  },
  "repository": {
    "type": "git",
    "directory": "https://github.com/thisVioletHydra/git-autocommit"
  },
  "homepage": "https://github.com/thisVioletHydra/git-autocommit#readme",
  "bugs": {
    "url": "https://github.com/thisVioletHydra/git-autocommit/issues",
    "email": "mrG0bliN.tm@gmail.com"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "author": "VioletHydra <mrG0bliN.tm@gmail.com>",
  "license": "MIT",
  "private": "false",
  "publisher": "Roman",
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Snippets"
  ],
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "yarn-check": "^0.0.3"
  }
}
